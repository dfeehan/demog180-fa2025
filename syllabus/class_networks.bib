@article{marwick_its_nodate,
  title = {`{{It}}'s Just Drama': Teen Perspectives on Conflict and Aggression in a Networked Era},
  shorttitle = {`{{It}}'s Just Drama'},
  author = {Marwick, Alice and {boyd}, danah},
  journal = {Journal of Youth Studies},
  volume = {0},
  number = {0},
  pages = {1--18},
  issn = {1367-6261},
  doi = {10.1080/13676261.2014.901493},
  urldate = {2014-05-05},
  abstract = {Contemporary youth conflict often plays out through social media like Facebook and Twitter. `Drama' is an emergent concept describing performative, interpersonal conflict that takes place in front of an active, engaged audience, often on social media. Using ethnographic data, this paper examines how American teenagers conceptualize the term drama; the relationship between drama and social media; and the implications drama has for understanding contemporary teenage conflict. The emic use of drama distances teens from practices conceptualized by adults as bullying or relational aggression, while acknowledging the role of the audience in social media interactions. Drama also serves to reinforce the conventional gendered norms of high school, perpetrating the systemic undervaluing of feminine subjects and re-inscribing heteronormativity. Understanding how drama operates helps illuminate how widespread use of social media among teenagers has altered dynamics of aggression and conflict.}
}

