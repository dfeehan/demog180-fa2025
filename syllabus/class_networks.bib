  author = {Chao, Li-Wei and Szrek, Helena and Peltzer, Karl and Ramlagan, Shandir and Fleming, Peter and Leite, Rui and Magerman, Jesswill and Ngwenya, Godfrey B. and Sousa Pereira, Nuno and Behrman, Jere},
  year = {2012},
