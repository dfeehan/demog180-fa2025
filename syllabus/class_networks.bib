@article{rastegari_estimation_2013,
  ids = {rastegari_estimation_2013-2,rastegari_estimation_2013-3},
  title = {The {{Estimation}} of {{Active Social Network Size}} of the {{Iranian Population}}},
  author = {Rastegari, Azam and {Haji-Maghsoudi}, Saiedeh and Haghdoost, Aliakbar and Shatti, Mohsen and Tarjoman, Termeh and Baneshi, Mohammad Reza},
  year = {2013},
