@article{service_cohort_1998,
  ids = {service_cohort_1998-1,service_cohort_1998-2,service_cohort_1998-3,service_cohort_1998-4,service_cohort_1998-5},
  title = {Cohort {{Size}} and {{Maximum Likelihood Estimation}} of {{Mortality Parameters}}},
  author = {Service, Philip M and Ochoa, Ricky and Valenzuela, Robert and Michieli, Charles A},
  year = {1998},
  month = jun,
  journal = {Experimental Gerontology},
  volume = {33},
  number = {4},
  pages = {331--342},
  issn = {0531-5565},
  doi = {10.1016/S0531-5565(97)00131-9},
  urldate = {2011-12-19},
  abstract = {The purpose of this study was to investigate the effects of cohort size on maximum likelihood estimates of mortality parameters. Recent experimental investigations have stressed the importance of large cohorts for detecting leveling off of mortality rates at older ages. In the present study, emphasis was placed on evaluation of relatively small cohorts (about 150--300 individuals). Deaths were simulated under the assumption of the frailty mortality model. Two different parameter sets that resulted in differences in mean life span of more than twofold were used for simulations. Our smallest cohorts yielded parameter estimates that had generally good statistical properties, but relatively large standard errors. For tests of hypotheses concerning equality of parameters among populations or experimental treatments, empirical standard errors (obtained from several cohorts) were preferable to asymptotic standard errors (obtained for single cohorts). In particular, empirical standard errors yielded reliable type I error rates.},
  keywords = {cohort size,frailty model,hypothesis testing,maximum likelihood,mortality parameter estimation}
}

