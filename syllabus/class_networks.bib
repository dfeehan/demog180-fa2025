  shorttitle = {Estimation of the Number of Injecting Drug Users Attending an Outreach Syringe-Exchange Program and Infection with Human Immunodeficiency Virus ({{HIV}}) and Hepatitis {{C}} Virus},
  author = {Caiaffa, Waleska Teixeira and Mingoti, Sueli Aparecida and Proietti, Fernando A and {Carneiro-Proietti}, Anna B{\'a}rbara and Silva, Rodrigo Carazolli and Lopes, Aline Cristine Souza and Doneda, Denise},
