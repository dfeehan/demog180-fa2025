@article{de_mel_measuring_2009,
  title = {Measuring Microenterprise Profits: {{Must}} We Ask How the Sausage Is Made?},
  shorttitle = {Measuring Microenterprise Profits},
  author = {{de Mel}, Suresh and McKenzie, David J. and Woodruff, Christopher},
  year = {2009},
  month = jan,
