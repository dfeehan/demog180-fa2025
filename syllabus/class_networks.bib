@article{fafchamps_using_2012,
  title = {Using {{PDA}} Consistency Checks to Increase the Precision of Profits and Sales Measurement in Panels},
  shorttitle = {Symposium on {{Measurement}} and {{Survey Design}}},
  author = {Fafchamps, Marcel and McKenzie, David and Quinn, Simon and Woodruff, Christopher},
  year = {2012},
