@article{kho_impact_2018,
  title = {Impact of {{Internet Access}} on {{Student Learning}} in {{Peruvian Schools}}},
  author = {Kho, Kevin and Lakdawala, Leah K and Nakasone, Eduardo},
  year = {2018},
  pages = {51},
  abstract = {We investigate the impacts of school-based internet access on pupil achievement in Peru, using a large panel of 5,903 public primary schools that gained internet connections during 20072014. We employ an event study approach and a trend break analysis that exploit variation in the timing of internet roll-out up to 5 years after installation. We find that internet access has a moderate, positive short-run impact on school-average standardized math scores, but importantly that this effect grows over time. We provide evidence that schools require time to adapt to internet access by hiring teachers with computer training and that this process is not immediate. These dynamics highlight the need for complementary investments to fully exploit new technological inputs and underscores the importance of using an extended evaluation window to allow the effects of school-based internet on learning to materialize.},
  langid = {english}
}

