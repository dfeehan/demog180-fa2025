@article{monk_unceasing_2021,
  title = {The {{Unceasing Significance}} of {{Colorism}}: {{Skin Tone Stratification}} in the {{United States}}},
  shorttitle = {The {{Unceasing Significance}} of {{Colorism}}},
  author = {Monk, Ellis P.},
  year = {2021},
  month = jan,
  journal = {Daedalus},
  volume = {150},
  number = {2},
  pages = {76--90},
  issn = {0011-5266, 1548-6192},
  doi = {10.1162/daed_a_01847},
  urldate = {2024-01-04},
  abstract = {Abstract             For many decades now, social scientists have documented immense ethnoracial inequalities in the United States. Much of this work is rooted in comparing the life chances, trajectories, and outcomes of African Americans to White Americans. From health to wealth and nearly every measure of well-being, success, and thriving one can find, White Americans remain ahead of Black Americans. What this focus on ethnoracial inequality between ``groups'' obscures, however, is long-standing skin tone inequality within groups. In this essay, I trace the trajectory of colorism and skin tone stratification in the United States over the past century. Next, I high-light the contemporary persistence of skin tone stratification, not only among African Americans, but among Latinx and Asian Americans as well. I conclude by arguing that future research on colorism will be essential to understand comprehensively the significance of race/ethnicity in a demographically shifting United States (such as immigration and ``multiraciality'').},
  langid = {english}
}

@book{monteiro_riscos_2004-2,
  ids = {monteiro_riscos_2004,monteiro_riscos_2004-1,monteiro_riscos_2004-3,monteiro_riscos_2004-4,monteiro_riscos_2004-5},
  title = {{Riscos {\`a} sobreviv{\^e}ncia de crian{\c c}as em {\'a}reas de favela (Belo Horizonte, Rio de Janeiro e S{\~a}o Paulo)}},
  author = {Monteiro, Mario Francisco Giani},
  year = {2004},
  abstract = {Os riscos {\`a} sobreviv{\^e}ncia de crian{\c c}as e adolescentes {\'e} um dos principais indicadores de demanda por aten{\c c}{\~a}o {\`a} sa{\'u}de porque evidenciam condi{\c c}{\~o}es que tornam a popula{\c c}{\~a}o mais vulner{\'a}vel {\`a} mortalidade prematura, que tem como causas principais doen{\c c}as e les{\~o}es que poderiam ser prevenidas e evitadas. O conhecimento e mensura{\c c}{\~a}o destes diferenciais permitem propor pol{\'i}ticas sociais e de sa{\'u}de para diminuir riscos entre popula{\c c}{\~o}es vulner{\'a}veis. A moradia em {\'a}reas de favela {\'e} um fator de risco importante, tanto por causa da grande parcela da popula{\c c}{\~a}o que mora em favelas nos munic{\'i}pios escolhidos (Belo Horizonte, Rio de Janeiro e S{\~a}o Paulo) formando os maiores complexos deste tipo da Am{\'e}rica Latina, como pelas condi{\c c}{\~o}es de vida desfavor{\'a}veis de seus habitantes freq{\"u}entemente associada com uma renda familiar baixa. A popula{\c c}{\~a}o moradora em {\'a}reas de favela est{\'a} submetida a riscos mais elevados de mortalidade prematura que a popula{\c c}{\~a}o residente em {\'a}reas fora da favela, nas tr{\^e}s capitais estudadas, chegando a ser o dobro para 5q0 em Belo Horizonte e para 20q0 em S{\~a}o Paulo. Quando observamos apenas a popula{\c c}{\~a}o de baixa renda (Figura 2), nas tr{\^e}s cidades os riscos aumentam para os moradores de favela, chegando a 171/1000 para 20q0 no Munic{\'i}pio de S{\~a}o Paulo. As estimativas de 5q0 e 20q0 para {\'a}reas de favela nos Munic{\'i}pios de Belo Horizonte, Rio de Janeiro e S{\~a}o Paulo, demonstram a grande vulnerabilidade das {\'a}reas de favela, com riscos excessivos de mortalidade prematura, evidenciando uma demanda urgente por servi{\c c}os de sa{\'u}de em {\'a}reas vulner{\'a}veis destas cidades mais industrializadas do Brasil.},
  langid = {portuguese}
}

