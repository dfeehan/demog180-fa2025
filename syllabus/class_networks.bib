@misc{adjiwanou_measuring_2020,
  title = {Measuring Excess Mortality during the {{COVID-19}} Pandemic in Low- and Lower-Middle Income Countries: The Need for Mobile Phone Surveys},
  shorttitle = {Measuring Excess Mortality during the {{COVID-19}} Pandemic in Low- and Lower-Middle Income Countries},
  author = {Adjiwanou, Viss{\'e}ho and Alam, Nurul and Alkema, Leontine and Asiki, Gershim and Bawah, Ayaga and B{\'e}guy, Donatien and Cetorelli, Valeria and Dube, Albert and Feehan, Dennis M. and Fisker, Ane Baerent and Gage, Anastasia and Garcia, Jenny and Gerland, Patrick and Guillot, Michel and Gupta, Aashish and Haider, M. Moinuddin and Helleringer, Stephane and Jasseh, Momodou and Kabudula, Chodziwadziwa and LeGrand, Thomas and Masquelier, Bruno and {Menashe-Oren}, Ashira and Moultrie, Tom A. and Queiroz, Bernardo L. and Reniers, Georges and Soura, Abdramane and Tim{\ae}us, Ian and You, Danzhen},
  year = {2020},
  month = may,
  publisher = {OSF},
  doi = {10.31235/osf.io/4bu3q},
  urldate = {2024-06-01},
  abstract = {In low income and lower-middle income countries, data from civil registration systems do not allow monitoring excess mortality during the COVID-19 pandemic. Rapid mobile phone surveys aimed at measuring mortality trends on a monthly basis are a realistic and safe option for filling that data gap. The data generated by mobile phone surveys can play a key role in better targeting areas or population groups most affected by the pandemic. They can also help monitor the impact of interventions and programs, and rapidly identify what works in mitigating the impact of COVID-19.},
  archiveprefix = {OSF},
  langid = {american},
  keywords = {Civil registration and vital statistics,COVID-19,Demography,Low income countries,Mobile phone surveys,Mortality,Survey data}
}

