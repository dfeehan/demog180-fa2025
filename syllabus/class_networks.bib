@article{bruch_aspirational_2018,
  title = {Aspirational Pursuit of Mates in Online Dating Markets},
  author = {Bruch, Elizabeth E. and Newman, M. E. J.},
  year = {2018},
  month = aug,
  journal = {Science Advances},
