@article{le_polain_de_waroux_characteristics_2018,
  title = {Characteristics of Human Encounters and Social Mixing Patterns Relevant to Infectious Diseases Spread by Close Contact: A Survey in {{Southwest Uganda}}},
  shorttitle = {Characteristics of Human Encounters and Social Mixing Patterns Relevant to Infectious Diseases Spread by Close Contact},
  author = {{le Polain de Waroux}, O. and Cohuet, S. and Ndazima, D. and Kucharski, A. J. and {Juan-Giner}, A. and Flasche, S. and Tumwesigye, E. and Arinaitwe, R. and {Mwanga-Amumpaire}, J. and Boum, Y. and Nackers, F. and Checchi, F. and Grais, R. F. and Edmunds, W. J. and Boum, Y 2nd},
  year = {2018},
  month = apr,
  journal = {BMC Infectious Diseases},
  volume = {18},
  number = {1},
  pages = {N.PAG-N.PAG},
  publisher = {BioMed Central},
  issn = {14712334},
  doi = {10.1186/s12879-018-3073-1},
  urldate = {2020-07-30},
  abstract = {{$<$}bold{$>$}Background: {$<$}/bold{$>$}Quantification of human interactions relevant to infectious disease transmission through social contact is central to predict disease dynamics, yet data from low-resource settings remain scarce.{$<$}bold{$>$}Methods: {$<$}/bold{$>$}We undertook a social contact survey in rural Uganda, whereby participants were asked to recall details about the frequency, type, and socio-demographic characteristics of any conversational encounter that lasted for {$\geq$}5~min (henceforth defined as 'contacts') during the previous day. An estimate of the number of 'casual contacts' (i.e. {$<$}\,5~min) was also obtained.{$<$}bold{$>$}Results: {$<$}/bold{$>$}In total, 566 individuals were included in the study. On average participants reported having routine contact with 7.2 individuals (range 1-25). Children aged 5-14~years had the highest frequency of contacts and the elderly ({$\geq$}65~years) the fewest (P\,{$<$}\,0.001). A strong age-assortative pattern was seen, particularly outside the household and increasingly so for contacts occurring further away from home. Adults aged 25-64~years tended to travel more often and further than others, and males travelled more frequently than females.{$<$}bold{$>$}Conclusion: {$<$}/bold{$>$}Our study provides detailed information on contact patterns and their spatial characteristics in an African setting. It therefore fills an important knowledge gap that will help more accurately predict transmission dynamics and the impact of control strategies in such areas.},
  keywords = {Close contact transmission,COMMUNICABLE diseases,COMPARATIVE studies,DISEASE prevalence,EVALUATION research,FAMILIES,INFECTIOUS disease transmission,Infectious diseases,MEDICAL cooperation,RESEARCH,RESEARCH methodology,RURAL population,Social contact,SOCIAL contact,SOCIAL interaction,SOCIAL skills,SOCIOECONOMIC factors,Survey,TRAVEL,Uganda,UGANDA}
}

