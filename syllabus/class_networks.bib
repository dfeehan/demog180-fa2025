@article{rust_variance_1996,
  ids = {rust_variance_1996-1,rust_variance_1996-10,rust_variance_1996-2,rust_variance_1996-3,rust_variance_1996-4,rust_variance_1996-5,rust_variance_1996-6,rust_variance_1996-7,rust_variance_1996-8,rust_variance_1996-9},
  title = {Variance Estimation for Complex Surveys Using Replication Techniques},
