@article{brewer_prostitution_2000,
  title = {Prostitution and the Sex Discrepancy in Reported Number of Sexual Partners},
  author = {Brewer, Devon D. and Potterat, John J. and Garrett, Sharon B. and Muth, Stephen Q. and Roberts, John M. and Kasprzyk, Danuta and Montano, Daniel E. and Darrow, William W.},
  year = {2000},
