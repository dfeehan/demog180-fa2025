  author = {Rice, Eric and Holloway, Ian W. and {Barman-Adhikari}, Anamika and Fuentes, Dahlia and Brown, C. Hendricks and Palinkas, Lawrence A.},
  year = {2014},
  month = aug,
  journal = {Field Methods},
  volume = {26},
