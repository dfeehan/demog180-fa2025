@article{bell_partner_2007,
  title = {Partner Naming and Forgetting: {{Recall}} of Network Members},
  shorttitle = {Partner Naming and Forgetting},
  author = {Bell, David C. and {Belli-McQueen}, Benedetta and Haider, Ali},
  year = {2007},
