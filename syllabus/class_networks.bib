  title = {Friends of {{Urban Men}}: {{An Assessment}} of {{Accuracy}} in {{Reporting Their Socioeconomic Attributes}}, {{Mutual Choice}}, and {{Attitude Agreement}}},
  shorttitle = {Friends of {{Urban Men}}},
  author = {Laumann, Edward O.},
