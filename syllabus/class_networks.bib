  title = {Reporting {{Bias}} and {{Sampling Errors}} in a {{Survey}} of a {{Rare Population Using Multiplicity Counting Rules}}},
  author = {Czaja, Ronald F. and Snowden, Cecelia B. and Casady, Robert J.},
