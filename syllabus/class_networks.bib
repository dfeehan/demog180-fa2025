@article{revilla_alternative_2018,
  title = {Alternative Methods for Selecting Web Survey Samples},
  author = {Revilla, Melanie and Ochoa, Carlos},
  year = {2018},
  month = jul,
  journal = {International Journal of Market Research},
  volume = {60},
