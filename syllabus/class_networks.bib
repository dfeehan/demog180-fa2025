@article{goerres_why_2007,
  title = {Why Are {{Older People More Likely}} to {{Vote}}? {{The Impact}} of {{Ageing}} on {{Electoral Turnout}} in {{Europe}}},
  shorttitle = {Why Are {{Older People More Likely}} to {{Vote}}?},
  author = {Goerres, Achim},
