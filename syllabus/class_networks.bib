  primaryclass = {math, stat},
  urldate = {2015-09-29},
  abstract = {Respondent-driven sampling is a popular technique for sampling hidden populations. This paper models respondent-driven sampling as a Markov process indexed by a tree. Theorems 1 and 2 study the asymptotic distribution of an estimator constructed from such a sample. The key technical difficulties stem from (i) the dependence between samples and (ii) the tree structure which characterizes the dependence. The theorems allow the growth rate of the tree to exceed one and suggest that this growth rate should not be too large. To illustrate the usefulness of these results beyond their obvious use, an example shows that in certain cases the sample average is preferable to inverse probability weighting. We provide a test statistic to distinguish between these two cases.},
  archiveprefix = {arXiv},
  keywords = {Mathematics - Statistics Theory,Statistics - Methodology}
}

